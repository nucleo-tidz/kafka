<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <RootNamespace>Nucleotidz.Kafka.Producer</RootNamespace>
	  <AssemblyName>Nucleotidz.Kafka.Producer</AssemblyName>
	  <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
	  <CodeAnalysisTreatWarningsAsErrors>false</CodeAnalysisTreatWarningsAsErrors>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <PackageId>Nucleotidz.Kafka.Producer</PackageId>
	  <!--<Version>1.0.4</Version>-->
	  <Authors>Ahmar Husain</Authors>
	  <Company>Nucleotidz</Company>
	  <Product>Nucleotidz.Kafka.Producer</Product>
	  <Description>Lightweight library to consume from kafka</Description>
	  <PackageProjectUrl>https://github.com/nucleo-tidz/kafka</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/nucleo-tidz/kafka</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>nucleotidz kafka Producer</PackageTags>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="7.0.0" />
    <ProjectReference Include="..\..\Abstraction\Nucleotidz.Kafka.Abstraction\Nucleotidz.Kafka.Abstraction.csproj" />
    <ProjectReference Include="..\..\SchemaRegistry\Nucleotidz.Kafka.SchemaRegistry\Nucleotidz.Kafka.SchemaRegistry.csproj" />
    <ProjectReference Include="..\..\Serializer\Nucleotidz.Kafka.Serializer\Nucleotidz.Kafka.Serializer.csproj" />
  </ItemGroup>

</Project>
